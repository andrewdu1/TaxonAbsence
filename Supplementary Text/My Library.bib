	journal = {Quaternary Science Reviews},
	author = {Katoh, Shigehiro and Nagaoka, Shinji and WoldeGabriel, Giday and Renne, Paul and Snow, Majorie G. and Beyene, Yonas and Suwa, Gen},
	month = sep,
