	author = {Lordkipanidze, David and Vekua, Abesalom and Ferring, Reid and Rightmire, G. Philip and Zollikofer, Christoph P.E. and Ponce de León, Marcia S. and Agusti, Jordi and Kiladze, Gocha and Mouskhelishvili, Alexander and Nioradze, Medea and Tappen, Martha},
