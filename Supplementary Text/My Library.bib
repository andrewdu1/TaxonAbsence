@article{fraser_late_2022,
	title = {Late quaternary biotic homogenization of {North} {American} mammalian faunas},
	volume = {13},
	copyright = {2022 The Author(s)},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-022-31595-8},
	doi = {10.1038/s41467-022-31595-8},
	abstract = {Biotic homogenization—increasing similarity of species composition among ecological communities—has been linked to anthropogenic processes operating over the last century. Fossil evidence, however, suggests that humans have had impacts on ecosystems for millennia. We quantify biotic homogenization of North American mammalian assemblages during the late Pleistocene through Holocene ({\textasciitilde}30,000 ybp to recent), a timespan encompassing increased evidence of humans on the landscape ({\textasciitilde}20,000–14,000 ybp). From {\textasciitilde}10,000 ybp to recent, assemblages became significantly more homogenous ({\textgreater}100\% increase in Jaccard similarity), a pattern that cannot be explained by changes in fossil record sampling. Homogenization was most pronounced among mammals larger than 1 kg and occurred in two phases. The first followed the megafaunal extinction at {\textasciitilde}10,000 ybp. The second, more rapid phase began during human population growth and early agricultural intensification ({\textasciitilde}2,000–1,000 ybp). We show that North American ecosystems were homogenizing for millennia, extending human impacts back {\textasciitilde}10,000 years.},
	language = {en},
	number = {1},
	urldate = {2022-07-11},
	journal = {Nature Communications},
	author = {Fraser, Danielle and Villaseñor, Amelia and Tóth, Anikó B. and Balk, Meghan A. and Eronen, Jussi T. and Andrew Barr, W. and Behrensmeyer, A. K. and Davis, Matt and Du, Andrew and Tyler Faith, J. and Graves, Gary R. and Gotelli, Nicholas J. and Jukar, Advait M. and Looy, Cindy V. and McGill, Brian J. and Miller, Joshua H. and Pineda-Munoz, Silvia and Potts, Richard and Shupinski, Alex B. and Soul, Laura C. and Kathleen Lyons, S.},
	month = jul,
	year = {2022},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Community ecology, Palaeoecology, Macroecology},
	pages = {3940},
	file = {Snapshot:C\:\\Users\\adu\\Zotero\\storage\\8JBLQRZY\\s41467-022-31595-8.html:text/html},
