@article{alroy_appearance_1994,
	title = {Appearance {Event} {Ordination}: {A} {New} {Biochronologic} {Method}},
	volume = {20},
	copyright = {Copyright © 1994 Paleontological Society},
	issn = {00948373},
	url = {http://www.jstor.org/stable/2401018},
	doi = {10.2307/2401018},
	abstract = {The fundamental goal of biochronology is ordering taxonomic first and last appearance events. The most useful biochronologic data are of the form "the first appearance event of one taxon predates the last appearance event of a second taxon" (FAE {\textless} LAE). FAE {\textless} LAE data sets are unusually reliable because they converge on a unique solution with greater sampling. The fact that the FAE of one taxon i {\textless} the LAE of another taxon j always can be inferred either if i is found lower than j in a stratigraphic section, or if i and j co-occur in at least one taxonomic list. Thus, FAE {\textless} LAE data accurately synthesize two disparate sources of information: routine biostratigraphic observations and taxonomic lists that may have no stratigraphic context. Appearance event ordination, the new method introduced here, is intended to summarize FAE {\textless} LAE data. The algorithm is founded on the following parsimony criterion: arrangements of FAEs and LAEs should always imply FAEi {\textless} LAEj when this is known, and otherwise imply LAEj {\textless} FAEi whenever possible. The technique differs from others related to correspondence analysis in its use of FAE {\textless} LAE data and explicit definition as a parsimony method. The algorithm is even more unique in that it uses different subsets of FAEi {\textless} LAEj statements at each iterative step, converging on separate sets of scores for the FAEs and LAEs. After arranging either the FAEs or the LAEs on the basis of their scores, the other set of scores can be discarded and the best arrangement of the remaining events can be inferred directly. An analysis of the Plio-Pleistocene mammalian record in the Lake Turkana region is used to illustrate the method. Biochronologic resolution on the order of 0.2-1.5 m.y. is achieved. The Turkana species lists by themselves demonstrate enough FAEi {\textless} LAEj relationships to resolve the basic biochronologic pattern, but stratigraphic information is still of great use.},
	number = {2},
	journal = {Paleobiology},
	author = {Alroy, John},
	month = apr,
	year = {1994},
	pages = {191--207},
}

