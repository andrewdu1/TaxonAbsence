	year = {2009},
	pmid = {19395769},
	keywords = {Animals, Animals, Wild, Ape Diseases, Body Temperature, Feces, Pan troglodytes, Sensitivity and Specificity, Sentinel Surveillance},
	pages = {542--546},
}

