	author = {Prat, Sandrine and Brugal, Jean-Philip and Tiercelin, Jean-Jacques and Barrat, Jean-Alix and Bohn, Marcel and Delagnes, Anne and Harmand, Sonia and Kimeu, Kamoya and Kibunjia, Mzalendo and Texier, Pierre-Jean and Roche, Hélène},
	month = aug,
