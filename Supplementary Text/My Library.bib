@incollection{behrensmeyer_building_2012,
	series = {Springer {Earth} {System} {Sciences}},
	title = {Building {Links} {Between} {Ecology} and {Paleontology} {Using} {Taphonomic} {Studies} of {Recent} {Vertebrate} {Communities}},
	copyright = {©2012 Springer-Verlag Berlin Heidelberg},
	isbn = {978-3-642-25037-8 978-3-642-25038-5},
	url = {http://link.springer.com/chapter/10.1007/978-3-642-25038-5_5},
	abstract = {Ecologists and paleontologists share a common interest in the natural cycles of life and death, but their viewpoints on living organisms and the processes that recycle or preserve their remains are different. The goal of this chapter is to explore the common ground between these fields through taphonomy, the sub-field of paleontology that examines how organisms are preserved as fossils. From studies of recent bone assemblages, taphonomists have assembled information about diversity and abundance, animal behaviour, predator–prey interactions, habitat utilization, mortality (how and where animals die), and nutrient recycling. These studies were initiated to strengthen paleontological understanding of the information content and biases in the fossil record, but the methods and discoveries of taphonomic research in modern ecosystems are also of potential value to ecologists. Both paleontology and ecology would benefit from increased exchange of ideas and perspectives, and this chapter provides examples showing why such exchange is worth pursuing and offers suggestions to encourage future dialogue and collaboration.},
	language = {en},
	urldate = {2015-05-18},
	booktitle = {Paleontology in {Ecology} and {Conservation}},
	publisher = {Springer Berlin Heidelberg},
	author = {Behrensmeyer, Anna K. and Miller, Joshua H.},
	editor = {Louys, Julien},
	year = {2012},
	keywords = {paleoecology, diversity, Paleontology, ecology, Taphonomy, Actualistic, Biogeosciences, Bone surveys, Conservation Biology/Ecology, Ecological baselines, Vertebrate},
	pages = {69--91},
}

