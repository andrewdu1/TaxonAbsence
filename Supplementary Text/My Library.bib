	urldate = {2023-06-20},
	journal = {Science},
	author = {Plummer, Thomas W. and Oliver, James S. and Finestone, Emma M. and Ditchfield, Peter W. and Bishop, Laura C. and Blumenthal, Scott A. and Lemorini, Cristina and Caricola, Isabella and Bailey, Shara E. and Herries, Andy I. R. and Parkinson, Jennifer A. and Whitfield, Elizabeth and Hertel, Fritz and Kinyanjui, Rahab N. and Vincent, Thomas H. and Li, Youjuan and Louys, Julien and Frost, Stephen R. and Braun, David R. and Reeves, Jonathan S. and Early, Emily D. G. and Onyango, Blasto and Lamela-Lopez, Raquel and Forrest, Frances L. and He, Huaiyu and Lane, Timothy P. and Frouin, Marine and Nomade, Sébastien and Wilson, Evan P. and Bartilol, Simion K. and Rotich, Nelson Kiprono and Potts, Richard},
