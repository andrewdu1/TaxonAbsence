	author = {Paul, Sudhir. R. and Jiang, Xing and Rai, Shesh Nath and Balasooriya, Uditha},
	year = {2004},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/sim.1764},
	keywords = {likelihood ratio test, PVC data, score test, treatment effect},
	pages = {1541--1554},
	file = {Snapshot:C\:\\Users\\adu\\Zotero\\storage\\U6SHBTWY\\sim.html:text/html},
}

