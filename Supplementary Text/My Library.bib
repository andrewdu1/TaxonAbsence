@article{tuomisto_how_2018,
	title = {How scientists perceive the evolutionary origin of human traits: {Results} of a survey study},
	volume = {8},
	copyright = {© 2018 The Authors.           Ecology and Evolution published by John Wiley \& Sons Ltd.},
	issn = {2045-7758},
	shorttitle = {How scientists perceive the evolutionary origin of human traits},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ece3.3887},
	doi = {10.1002/ece3.3887},
	abstract = {Various hypotheses have been proposed for why the traits distinguishing humans from other primates originally evolved, and any given trait may have been explained both as an adaptation to different environments and as a result of demands from social organization or sexual selection. To find out how popular the different explanations are among scientists, we carried out an online survey among authors of recent scientific papers in journals covering relevant fields of science (paleoanthropology, paleontology, ecology, evolution, human biology). Some of the hypotheses were clearly more popular among the 1,266 respondents than others, but none was universally accepted or rejected. Even the most popular of the hypotheses were assessed “very likely” by {\textless}50\% of the respondents, but many traits had 1–3 hypotheses that were found at least moderately likely by {\textgreater}70\% of the respondents. An ordination of the hypotheses identified two strong gradients. Along one gradient, the hypotheses were sorted by their popularity, measured by the average credibility score given by the respondents. The second gradient separated all hypotheses postulating adaptation to swimming or diving into their own group. The average credibility scores given for different subgroups of the hypotheses were not related to respondent's age or number of publications authored. However, (paleo)anthropologists were more critical of all hypotheses, and much more critical of the water-related ones, than were respondents representing other fields of expertise. Although most respondents did not find the water-related hypotheses likely, only a small minority found them unscientific. The most popular hypotheses were based on inherent drivers; that is, they assumed the evolution of a trait to have been triggered by the prior emergence of another human-specific behavioral or morphological trait, but opinions differed as to which of the traits came first.},
	language = {en},
	number = {6},
	urldate = {2018-07-30},
	journal = {Ecology and Evolution},
	author = {Tuomisto, Hanna and Tuomisto, Matleena and Tuomisto, Jouni T.},
	month = mar,
	year = {2018},
	keywords = {alternative hypotheses, aquatic ape hypothesis, bipedalism, encephalization, human evolution, nakedness},
	pages = {3518--3533},
}

@article{turvey_new_2018,
	title = {New genus of extinct {Holocene} gibbon associated with humans in {Imperial} {China}},
	volume = {360},
	copyright = {Copyright © 2018 The Authors, some rights reserved; exclusive licensee American Association for the Advancement of Science. No claim to original U.S. Government Works. http://www.sciencemag.org/about/science-licenses-journal-article-reuseThis is an article distributed under the terms of the Science Journals Default License.},
	issn = {0036-8075, 1095-9203},
	url = {http://science.sciencemag.org/content/360/6395/1346},
	doi = {10.1126/science.aao4903},
	abstract = {The noblewoman's ape
Human activities are causing extinctions across a wide array of taxa. Yet there has been no evidence of humans directly causing extinction among our relatives, the apes. Turvey et al. describe a species of gibbon found in a 2200- to 2300-year-old tomb ascribed to a Chinese noblewoman. This previously unknown species was likely widespread, may have persisted until the 18th century, and may be the first ape species to have perished as a direct result of human activities. This discovery may also indicate the existence of unrecognized primate diversity across Asia.
Science, this issue p. 1346
Although all extant apes are threatened with extinction, there is no evidence for human-caused extinctions of apes or other primates in postglacial continental ecosystems, despite intensive anthropogenic pressures associated with biodiversity loss for millennia in many regions. Here, we report a new, globally extinct genus and species of gibbon, Junzi imperialis, described from a partial cranium and mandible from a {\textasciitilde}2200- to 2300-year-old tomb from Shaanxi, China. Junzi can be differentiated from extant hylobatid genera and the extinct Quaternary gibbon Bunopithecus by using univariate and multivariate analyses of craniodental morphometric data. Primates are poorly represented in the Chinese Quaternary fossil record, but historical accounts suggest that China may have contained an endemic ape radiation that has only recently disappeared.
A previously unknown gibbon species found in a Chinese tomb suggests that past Asian ape diversity has been lost.
A previously unknown gibbon species found in a Chinese tomb suggests that past Asian ape diversity has been lost.},
	language = {en},
	number = {6395},
	urldate = {2018-06-21},
	journal = {Science},
	author = {Turvey, Samuel T. and Bruun, Kristoffer and Ortiz, Alejandra and Hansford, James and Hu, Songmei and Ding, Yan and Zhang, Tianen and Chatterjee, Helen J.},
	month = jun,
	year = {2018},
	pages = {1346--1349},
}

@article{townsend_peterson_transferability_2007,
	title = {Transferability and model evaluation in ecological niche modeling: a comparison of {GARP} and {Maxent}},
	volume = {30},
	issn = {1600-0587},
	shorttitle = {Transferability and model evaluation in ecological niche modeling},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.0906-7590.2007.05102.x/abstract},
	doi = {10.1111/j.0906-7590.2007.05102.x},
	abstract = {We compared predictive success in two common algorithms for modeling species’ ecological niches, GARP and Maxent, in a situation that challenged the algorithms to be general – that is, to be able to predict the species’ distributions in broad unsampled regions, here termed transferability. The results were strikingly different between the two algorithms – Maxent models reconstructed the overall distributions of the species at low thresholds, but higher predictive levels of Maxent predictions reflected overfitting to the input data; GARP models, on the other hand, succeeded in anticipating most of the species’ distributional potential, at the cost of increased (apparent, at least) commission error. Receiver operating characteristic (ROC) tests were weak in discerning models able to predict into broad unsampled areas from those that were not. Such transferability is clearly a novel challenge for modeling algorithms, and requires different qualities than does predicting within densely sampled landscapes – in this case, Maxent was transferable only at very low thresholds, and biases and gaps in input data may frequently affect results based on higher Maxent thresholds, requiring careful interpretation of model results.},
	language = {en},
	number = {4},
	urldate = {2016-11-06},
	journal = {Ecography},
	author = {Townsend Peterson, A. and Papeş, Monica and Eaton, Muir},
	month = aug,
	year = {2007},
	pages = {550--560},
}

@article{ugland_lognormal_1982,
	title = {Lognormal {Distributions} and the {Concept} of {Community} {Equilibrium}},
	volume = {39},
	issn = {0030-1299},
	url = {http://www.jstor.org/stable/3544482},
	doi = {10.2307/3544482},
	abstract = {The lognormal distribution of individuals among species fits many sets of ecological data remarkably well. Dispute exists however, as to whether this distribution is a mathematical artifact of large numbers or whether it has a biological meaning. In analysing general lognormal curves we argue that the characteristics discovered by Preston relating to the canonical lognormal are in fact merely mathematical properties of the whole family of lognormal curves. We develop a model that assumes that a community is composed of groups of species each differently adapted to a habitat. By integrating the distributions of these groups a lognormal distribution results if the community is in equilibrium. If the community is disturbed however, many species change their abundance and then species abundance curves show multiple maxima and no longer conform to a single lognormal curve. /// Логонормальное распределение особей отдельных видов хорошо соответствует результатам разных серий экологических наблюдений. Однако, остается дискуссионным, является ли такое распределение математическим артефактом большого числа повторностей, либо оно имеет биологический смысл. При анализе общих логнормальных кривых мы полагаем, что характеристики, данные Престоном, близкие к каноническим логнормальным, имеют лишь математические особенности, присущие всей группе логнормальных кривых. Мы разработали модель, в которой сообщество состоит из групп видов, каждый из которых по-разному адаптирован к местообитанию. При интеграции распределения этих групп получается логнормальное распределение при условии, если сообщество уравновешено. Если сообщество нарущено, у многих видов меняется обилие и, таким образом, кривые видового обилия имеют многочисленные максимумы и больше не сводятся к единственной логнормальной кривой.},
	number = {2},
	urldate = {2016-12-20},
	journal = {Oikos},
	author = {Ugland, Karl I. and Gray, John S.},
	year = {1982},
	pages = {171--178},
}

